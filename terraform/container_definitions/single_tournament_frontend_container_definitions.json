[ 
   {
      "name" : "single-tournament-frontend-container",
      "image" : "${account_id}.dkr.ecr.ap-northeast-1.amazonaws.com/single-tournament-frontend:latest",
      "essential" : true,
      "portMappings" : [
         {
            "protocol" : "tcp",
            "containerPort" : 8080,
            "hostPort" : 8080
         }
      ],
      "environment" : [
         {
            "name" : "NODE_ENV",
            "value" : "${environment}"
         }
      ]
   }
]